FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

regions
(
    boxToCell
    {	
    	box (-1 -1 0.12238) (1 1 0.146);
    	fieldValues
        (
           volScalarFieldValue alpha.water 0 //we set our block of air into the domain
        );
    }
    
);


